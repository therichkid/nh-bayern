    "vue-gtag": "^1.16.1",
    "vue-router": "^3.6.5",
    "vuetify": "^2.7.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^8.53.0",
